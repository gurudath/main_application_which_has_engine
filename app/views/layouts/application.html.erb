<!DOCTYPE html>
<html>
<head>
  <title>Gurudath Application</title>
  <%= stylesheet_link_tag    "application" %>
  <%= javascript_include_tag "application" %>
  <script src='http://ajax.googleapis.com/ajax/libs/jquery/1.4.2/jquery.min.js' type='text/javascript'></script>
  <script type="text/javascript" src="http://player.longtailvideo.com/jwplayer.js"></script>
  <script type="text/javascript" src="http://code.jquery.com/jquery-latest.js/jquery.js"></script>
  <!--script src='/javascripts/jwplayer.js' type='text/javascript'></script-->
  <!--script src='/javascripts/swfobject.js' type='text/javascript'></script-->
  <script src='http://ajax.googleapis.com/ajax/libs/swfobject/2.2/swfobject.js' type='text/javascript'></script>

  <!--script type="text/javascript" src="/javascripts/html5gallery/html5gallery.js"></script-->
  <script type="text/javascript" src="/tinymce/jscripts/tiny_mce/tiny_mce.js"></script>
  <script type="text/javascript" src="/javascripts/jquery.min.js"></script>
  <!--script type="text/javascript" src="/javascripts/jquery.easing.js"></script>
  <script type="text/javascript" src="/javascripts/jquery.slidorion.min.js"></script>
  <link href="/stylesheets/slidorion.css"  rel="stylesheet" type="text/css" />
  <link href='http://fonts.googleapis.com/css?family=Yanone+Kaffeesatz:400,200,700' rel='stylesheet' type='text/css'-->
  <%= csrf_meta_tags %>
</head>
<body>
<div id="container" style="width:100%">
  <div id="header" style="background-color:#FFA500;">
    <h1 style="margin-bottom:0;text-align: center;">Made By BN Gurudath</h1></div>
     <% if @current_user %>
         Logged in as <%= @current_user.email_id %>
         <%= link_to "Log out", :controller=>"login",:action=>"logout"  %>
      <% else %>
      <% end %>
      <%=render :partial=>"/layouts/menu"%>
      <div id="content" style="background-color:#EEEEEE;height:200px;width:100%;float:left;">
       <%= yield %>
      </div>
   <div id="footer" style="background-color:#FFA500;clear:both;text-align:center;">gurudath.1989@gmail.com</div>
</div>
</body>
</html>
